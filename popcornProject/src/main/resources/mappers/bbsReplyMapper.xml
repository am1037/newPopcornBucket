<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BbsReplyDAO">
    <select id="selectAll" resultType="com.java4.popcorn.crawling.MovieScreenVO">
        select * from multi.movie_screen
    </select>
    <insert id="insertOne">
        insert into multi.movie_screen
        values(#{title}, #{theater}, #{screen}, #{time}, #{date}, #{theater_id})
    </insert>
</mapper>